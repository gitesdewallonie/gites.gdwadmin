<configure xmlns="http://namespaces.zope.org/zope"
           xmlns:browser="http://namespaces.zope.org/browser"
           i18n_domain="gites">

    <browser:page
      for="*"
      name="mapBlacklist"
      permission="zope2.View"
      class=".mapblacklist.MapBlacklist"
      template="templates/mapblacklist.pt"
      />

    <browser:page
      for="*"
      name="mapBlacklistLiveSearch"
      permission="zope2.View"
      class=".mapblacklist.MapBlacklistLiveSearch"
      />

    <browser:page
      for="*"
      name="mapBlacklistSearchResult"
      permission="zope2.View"
      class=".mapblacklist.MapBlacklistSearchResult"
      template="templates/mapblacklist_searchresult.pt"
      />

    <browser:page
      for="*"
      name="mapBlacklistRemoveData"
      permission="zope2.View"
      class=".mapblacklist.MapBlacklist"
      attribute="removeData"
      />

    <browser:page
      for="*"
      name="mapBlacklistAddData"
      permission="zope2.View"
      class=".mapblacklist.MapBlacklist"
      attribute="addData"
      />

    <browser:page
      for="*"
      name="mapBlacklistRowData"
      permission="zope2.View"
      class=".mapblacklist.MapBlacklist"
      template="templates/mapblacklist_row.pt"
      />

</configure>
