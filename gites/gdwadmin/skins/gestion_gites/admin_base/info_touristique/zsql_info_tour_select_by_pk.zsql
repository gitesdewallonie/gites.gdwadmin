select
   infotour_pk,
   infotour_nom,
   infotour_adresse,
   infotour_url,
   infotour_gps_long,
   infotour_gps_lat,
   infotour_localite,
   infotour_commune_fk,
   infotour_type_infotour_fk,
   com_nom,
   com_mais_fk,
   com_cp,
   mais_nom,
   typinfotour_nom_fr
from
   info_touristique,
   commune,
   maison_tourisme,
   type_info_touristique
where
   com_pk=infotour_commune_fk
   and
   mais_pk=com_mais_fk
   and
   typinfotour_pk=infotour_type_infotour_fk
   and
   infotour_pk=<dtml-sqlvar infotour_pk type="int">
