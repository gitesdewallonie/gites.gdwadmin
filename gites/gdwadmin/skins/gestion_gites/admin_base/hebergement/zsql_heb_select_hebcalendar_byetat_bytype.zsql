select 
   count(heb_pk)
from 
   hebergement,
   proprio
where 
   heb_calendrier_proprio = <dtml-sqlvar etat type="string">
   and
   heb_typeheb_fk in <dtml-var type>
   and
   proprio.pro_pk = hebergement.heb_pro_fk
   and
   proprio.pro_etat = True
