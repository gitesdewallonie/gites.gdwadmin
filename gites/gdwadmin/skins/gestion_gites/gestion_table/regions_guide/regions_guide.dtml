<!DOCTYPE HTML PUBLIC "-//W3C//DTD HTML 4.0 Transitional//EN">
<html>
<head>
<title> Gites wallons : gestion table region_guide </TITLE>
<meta http-equiv="content-type" content="text/html; charset=UTF-8" >
<dtml-call "RESPONSE.setHeader('Content-Type', 'text/html;charset=UTF-8')">
<meta name="author" content="Affinitic">
<link href="gdwadmin.css" type="text/css" rel="stylesheet" />
<script language=JavaScript src="gites_wallons_script.js"></script>
</head>
<body class="gdwadmin-content">
<span class="titre_page">Gestion de la table region_guide</span><br><br>

<form name="formulaire" action="traiter_modifsup_region_guide" method="get">
<input type="hidden" name="operation" value="">
<table width="95%" border="1" cellspacing="0" bordercolor="#EEEFEE">
	<tr>
		<td colspan="3" align="left" bgcolor="#006666">
			<span class="titre_encodage">
			Liste des Régions
			</span>
			</font>
		</td>
	</tr>
	<tr>
		<td width="15">&nbsp;</td>
		<td><b>Nom</b></td>
		<td><b>Code</b></td>
	</tr>
	<dtml-in zsql_select_all_region_guide>
		<dtml-if sequence-odd>
			<tr>
		<dtml-else>
			<tr bgcolor="#E6FED9">
		</dtml-if>
		<td width="15"><input type="radio" name="freg_pk" value="<dtml-var reg_pk>"></td>
		<td><dtml-var reg_nom></td>
		<td><dtml-var reg_num></td>
	</tr>
	</dtml-in>
	<tr>
		<td colspan="3" align="center">
			<dtml-var bouton_modifier>
		    &nbsp;&nbsp;&nbsp;
			<dtml-var bouton_supprimer>
		</td>
	</tr>
</table>
</form>

<br><br>

<form name="formulaire_creation" action="traiter_insertion_region_guide" method="get">
<table border="1" cellspacing="0" cellpadding="0" width="75%" bordercolor="#EEEFEE">
	<tr>
		<td colspan="3" align="left" bgcolor="#006666">
			<span class="titre_encodage">
			Créer une nouvelle Région du guide
			</span>
			</font>
		</td>
	</tr>
	<tr>
		<td>Nom</td>
		<td><input type="text" name="freg_nom" value=""></td>
	</tr>
	<tr>
		<td>Code</td>
		<td><input type="text" name="freg_num" value=""></td>
	</tr>
	<tr>
		<td colspan="2" align="center">
			<dtml-var bouton_creer>
		</td>
	</tr>
</table>
</form>

</body>
</html>
