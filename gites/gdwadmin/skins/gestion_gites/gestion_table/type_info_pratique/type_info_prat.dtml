<body>
<span class="titre_page">Gestion de la table des types d'informations pratiques</span>
<br /><br />
<form name="formulaire" action="mod_sup_type_info_prat" method="get">
   <input type="hidden" name="operation" value="">
   <table width="95%" border="1" cellspacing="0" bordercolor="#EEEFEE">
      <tr>
         <td colspan="6" align="left" bgcolor="#006666">
         <span class="titre_encodage">
            Liste des types d'informations pratiques
         </span>
         </font>
         </td>
      </tr>
      <tr>
         <td width="15">&nbsp;</td>
         <td><b>Type FR</b></td>
         <td><b>Type NL</b></td>
         <td><b>Type EN</b></td>
         <td><b>Type IT</b></td>
         <td><b>Type DE</b></td>
      </tr>
         <dtml-in "admin_base.info_pratique.zsql_type_info_prat_select_all()">
            <dtml-if sequence-odd>
               <tr>
            <dtml-else>
               <tr bgcolor="#E6FED9">
            </dtml-if>
            <td width="15">
               <input type="radio" 
                      name="typinfoprat_pk" 
                      value="<dtml-var typinfoprat_pk>">
            </td>		
            <td>
               <dtml-var typinfoprat_nom_fr>
            </td>
            <td>
               <dtml-var typinfoprat_nom_nl>
            </td>
            <td>
               <dtml-var typinfoprat_nom_en>
            </td>
            <td>
               <dtml-var typinfoprat_nom_it>
            </td>
            <td>
               <dtml-var typinfoprat_nom_de>
            </td>
            </tr>
         </dtml-in>
      <tr>
         <td colspan="6" align="center">
            <dtml-var bouton_modifier>
            &nbsp;&nbsp;&nbsp;
            <dtml-var bouton_supprimer>
         </td>
      </tr>
   </table>
</form>

<br /><br />

<form name="formulaire_creation" 
      action="traiter_insertion_type_info_prat" 
      method="get">
   <table border="1" 
          cellspacing="0" 
          cellpadding="0" 
          width="75%" 
          bordercolor="#EEEFEE">
      <tr>
         <td colspan="3" align="left" bgcolor="#006666">
            <span class="titre_encodage">
               Cr√©er un nouveau type d'information pratique
            </span>
         </td>
      </tr>	
      <tr>
         <td>Type FR</td>
         <td>
            <input type="text" 
                   name="typinfoprat_nom_fr" 
                   value=""
                   size="50">
         </td>
      </tr>
      <tr>
         <td>Type NL</td>
         <td>
            <input type="text" 
                   name="typinfoprat_nom_nl" 
                   value=""
                   size="50">
         </td>
      </tr>
      <tr>
         <td>Type DE</td>
         <td>
            <input type="text" 
                   name="typinfoprat_nom_en" 
                   value=""
                   size="50">
         </td>
      </tr>
      <tr>
         <td>Type IT</td>
         <td>
            <input type="text" 
                   name="typinfoprat_nom_it" 
                   value=""
                   size="50">
         </td>
      </tr>
      <tr>
         <td>Type EN</td>
         <td>
            <input type="text" 
                   name="typinfoprat_nom_de" 
                   value=""
                   size="50">
         </td>
      </tr>
      <tr>
         <td colspan="2" align="center">
            <dtml-var bouton_creer>
         </td>
      </tr>
   </table>
</form>	

